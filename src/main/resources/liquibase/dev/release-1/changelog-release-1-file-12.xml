    <changeSet id="media-eug-39" logicalFilePath="path-independent" author="eugene">
        <comment>Creating table: media_parameter_conf</comment>
        <createTable tableName="media_parameter_conf" schemaName="apps">
            <column name="id" type="number">
                <constraints nullable="false"/>
            </column>

            <column name="param_id" type="number">
                <constraints nullable="false"/>
            </column>

            <column name="source_system_code" type="varchar(20)">
