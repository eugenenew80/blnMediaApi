    <changeSet id="media-eug-20" logicalFilePath="path-independent" author="eugene">
        <comment>Creating table: media_connection_config</comment>
        <createTable tableName="media_connection_config" schemaName="apps">
            <column name="id" type="number">
                <constraints nullable="false"/>
            </column>

            <column name="name" type="varchar(100)">
                <constraints nullable="false"/>
            </column>

            <column name="source_system_code" type="varchar(20)">
                <constraints nullable="false"/>
            </column>

            <column name="con_type" type="varchar(20)">
                <constraints nullable="false"/>
            </column>

            <column name="url" type="varchar(300)">
                <constraints nullable="false"/>
            </column>

            <column name="user_name" type="varchar(20)">
                <constraints nullable="true"/>
            </column>

            <column name="pwd" type="varchar(20)">
                <constraints nullable="true"/>
            </column>

            <column name="create_date" type="TIMESTAMP">
                <constraints nullable="true"/>
            </column>

            <column name="last_update_date" type="TIMESTAMP">
                <constraints nullable="true"/>
            </column>

            <column name="create_by" type="number">
                <constraints nullable="true"/>
            </column>

            <column name="last_update_by" type="number">
                <constraints nullable="true"/>
            </column>
        </createTable>
    </changeSet>

	<changeSet id="media-eug-21" logicalFilePath="path-independent" author="eugene">
	    <comment>Creating primary key for table: media_connection_config</comment>
	    <addPrimaryKey 
	            columnNames="id"
	            constraintName="pk_media_connection_config"
	            schemaName="apps"
	            tableName="media_connection_config"/>
	</changeSet>
	
	<changeSet id="media-eug-22" logicalFilePath="path-independent" author="eugene">
